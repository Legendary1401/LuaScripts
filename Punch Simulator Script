_G.autoTrain = false
_G.autoFight = false
_G.autoEggHatch = false
_G.autoMiner = false


spawn(function()
	AutoTrainON.MouseButton1Click:Connect(function()
		_G.autoTrain = true
		print("autoTrain On")

		while _G.autoTrain == true do
			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DamageIncreaseOnClickEvent"):FireServer()
			wait(0.1)
		end
	end)
end)

spawn(function()
	AutoTrainOFF.MouseButton1Click:Connect(function()
		_G.autoTrain = false
		print("autoTrain Off")
	end)
end)

spawn(function()
	AutoFightON.MouseButton1Click:Connect(function()
		_G.autoFight = true
		print("autoFight On")

		while _G.autoFight == true do
			local args = {
				[1] = true
			}
			wait(0.5)
			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("PushEvent"):FireServer(unpack(args))
		end
	end)
end)

spawn(function()
	AutoFightOFF.MouseButton1Click:Connect(function()
		_G.autoFight = false
		print("autoFight Off")
	end)
end)

spawn(function()
	AutoEggHatcherON.MouseButton1Click:Connect(function()
		_G.autoEggHatch = true
		print("AutoEggHatch On")

		while _G.autoEggHatch == true do
			local args = {
				[1] = "1"
			}
			wait(0.5)
			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("PlayerPressedKeyOnEgg"):FireServer(unpack(args))
		end
	end)
end)

spawn(function()
	AutoEggHatcherOFF.MouseButton1Click:Connect(function()
		_G.autoEggHatch = false
		print("AutoEggHatch Off")
	end)
end)

spawn(function()
	AutoMinerON.MouseButton1Click:Connect(function()
		_G.autoMiner = true
		print("AutoMiner On")

		while _G.autoMiner == true do
			local arg = {
				[1] = "Info"
			}

			game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("MinerCurrency"):InvokeServer(unpack(arg))

			local args = {
				[1] = "Coins",
				[2] = TextBox.Text
			}
			wait(0.5)
			game:GetService("ReplicatedStorage"):WaitForChild("CollectedCurrency"):FireServer(unpack(args))

		end
	end)
end)

spawn(function()
	AutoMinerOFF.MouseButton1Click:Connect(function()
		_G.autoMiner = false
		print("AutoMiner Off")
	end)
end)
